{
  "name": "react-horizontal-timeline",
  "version": "0.1.1",
  "description": "A horizontal timeline component made using react",
  "main": "./dist/react-horizontal-timeline.js",
  "scripts": {
    "start": "node tools/server.js",
    "build": "./node_modules/.bin/webpack --config tools/webpack.config.prod.js",
    "build:demos": "./node_modules/.bin/webpack --config tools/webpack.config.demos.prod.js",
    "prepublish": "npm run build",
    "lint": "./node_modules/.bin/eslint --config tools/lint-config.js src tools demos",
    "test": "ava",
    "test:cover": "nyc ava",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jckdrpr/react-horizontal-timeline"
  },
  "keywords": [
    "react",
    "timeline",
    "horizontal",
    "react-motion",
    "react-component"
  ],
  "author": "Sherub Thakur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jckdrpr/react-horizontal-timeline/issues"
  },
  "homepage": "https://github.com/jckdrpr/react-horizontal-timeline",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "ava": {
    "verbose": true,
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!dist/**/*",
      "!coverage/**/*",
      "!src/**/*.test.js"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "./tools/test-helpers.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-register": "^6.9.0",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.1",
    "radium": "^0.17.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-icons": "^2.0.1",
    "react-motion": "^0.4.2",
    "react-swipeable-views": "^0.6.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.3.21",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bootstrap-webpack": "0.0.5",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.6.0",
    "eslint-plugin-react": "^5.1.1",
    "estraverse-fb": "^1.3.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "imports-loader": "^0.6.5",
    "jsdom": "^9.4.1",
    "karma-spec-reporter": "0.0.26",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "nyc": "^7.0.0",
    "react-addons-test-utils": "^15.0.2",
    "react-transform-hmr": "^1.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  }
}
